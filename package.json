{
	"name": "red_tetris",
	"version": "1.0.0",
	"description": "full stack javascript project",
	"main": "src/server/index.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"build": "webpack --mode production",
		"start": "node src/server/index.js",
		"client": "webpack-dev-server --hot",
		"server": "nodemon src/server/index.js",
		"coverage": "jest --watchAll"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/woolimi/red_tetris.git"
	},
	"author": "wpark",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/woolimi/red_tetris/issues"
	},
	"homepage": "https://github.com/woolimi/red_tetris#readme",
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"lodash": "^4.17.20",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0",
		"semantic-ui-css": "^2.4.1",
		"semantic-ui-react": "^2.0.0",
		"socket.io": "^2.3.0",
		"socket.io-client": "^2.3.1",
		"string-to-color": "^2.2.2",
		"styled-components": "^5.2.0",
		"supertest": "^6.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"@testing-library/jest-dom": "^5.11.6",
		"@testing-library/react": "^11.1.0",
		"@types/jest": "^26.0.15",
		"@types/socket.io": "^2.1.11",
		"@types/socket.io-client": "^1.4.34",
		"babel-jest": "^26.6.1",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"concurrently": "^5.3.0",
		"css-loader": "^5.0.0",
		"error-overlay-webpack-plugin": "^0.4.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^4.5.0",
		"jest": "^26.6.1",
		"jest-fetch-mock": "^3.0.3",
		"jest-styled-components": "^7.0.3",
		"nodemon": "^2.0.6",
		"socket.io-mock": "^1.3.1",
		"style-loader": "^2.0.0",
		"url-loader": "^4.1.1",
		"webpack": "^5.1.3",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0"
	},
	"browserslist": [
		"last 1 chrome version",
		"last 1 firefox version",
		"last 1 safari version"
	],
	"jest": {
		"moduleFileExtensions": [
			"js",
			"jsx"
		],
		"moduleDirectories": [
			"node_modules"
		],
		"moduleNameMapper": {
			"\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/test/__mocks__/fileMock.js"
		},
		"setupFiles": [
			"<rootDir>/test/setup.js"
		],
		"collectCoverage": true,
		"coverageThreshold": {
			"global": {
				"statements": 70,
				"functions": 70,
				"lines": 70,
				"branches": 50
			}
		}
	}
}
